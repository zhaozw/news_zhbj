 * 组图对应的Javabean
 */
public class ArrPicBean {
    public int id;
    public String listimage;
    public String title;

    @Override
    public String toString() {
        return "ArrPicBean{" +
                "id=" + id +
                ", listimage='" + listimage + '\'' +
                ", title='" + title + '\'' +
                '}';
    }

    public ArrPicBean(int id, String listimage, String title) {
        this.id = id;
        this.listimage = listimage;
        this.title = title;
    }

    public ArrPicBean() {
        super();
    }
}
